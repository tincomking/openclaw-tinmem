{
  "name": "openclaw-tinmem",
  "version": "1.0.1",
  "description": "A powerful memory system for OpenClaw AI assistant, combining structured categorization, hybrid retrieval, and intelligent deduplication.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "tinmem": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:coverage": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "openclaw",
    "memory",
    "lancedb",
    "ai",
    "agent",
    "vector-search",
    "bm25",
    "embedding"
  ],
  "author": "openclaw-tinmem contributors",
  "license": "Apache-2.0",
  "dependencies": {
    "@lancedb/lancedb": "^0.14.0",
    "apache-arrow": "^17.0.0",
    "commander": "^12.1.0",
    "zod": "^3.23.8",
    "chalk": "^5.3.0",
    "ora": "^8.1.1",
    "table": "^6.8.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.7.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "@types/jest": "^29.5.14"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
